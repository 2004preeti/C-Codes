//swap alternate
void swapAlternate(int *arr, int size)
{
   if(size%2==0)
   {
       int i=0;
     while(i<size)
     {
         swap(arr[i],arr[i+1]);
         i+=2;
     }
   }
   else{
       int i=0;
     while(i<size-1)
     {
         swap(arr[i],arr[i+1]);
         i+=2;
     }
   }
   
    
    //Write your code here
}
